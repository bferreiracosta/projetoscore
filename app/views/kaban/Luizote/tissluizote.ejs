<%- include('../cabecalhokaban'); %>
<% if(id.length > 0) { %>
    <% for(var i = 0; i < id.length; i++) {%>
       
<h2> TISS Luizote</h2>
<a href="addtissluizote?id=<%=id[i].id_usuario %>"><button class="btn btn-success"  >Atualizar TISS</button></a>
<br>
<br>
    <table class="table table-striped" id="mytable">
        <input name="usuario" type="hidden" value="<%=id[i].id_usuario %>">
        
        <thead>
            <tr>
                <th><i class="fas fa-edit"></i></th>
                <th>id</th>
                <th>Nome</th>
                <th>Avaliação Atividades Basicas</th>
                <th>Avaliação Suporte Ventilatorio</th>
                <th>Avaliação Suporte Cardiovascular</th>
                <th>Avaliação Suporte Renal</th>
                <th>Avaliação Suporte Neurologico</th>
                <th>Avaliação Suporte Metabólico</th>
                <th>Avaliação Intervenções</th>
                <th>Resultado tiss</th>
                <th>Unidade</th>   
            </tr>
        </thead>
        <tbody>   
            <% if(tiss.length > 0) { %>
                <% for(var j = 0; j < tiss.length; j++) {%>
                    
            <tr>
                <td>
					<a href="edittissluizote/<%=id[i].id_usuario %>/?id=<%=tiss[j].idpaciente %>"class="btn btn-sm btn-info edit"> Editar</a>
				</td>
                <td><label for="">ID: </label><%=tiss[j].idpaciente %></td>
                <td><label hidden for="">Nome: </label><%=tiss[j].nome %></td>
                <td><label hidden for="">Dispositivo: </label><%=tiss[j].atividadesbasica %></td>
                <td><label hidden for="">fluxo_de_o2: </label><%=tiss[j].suporteventilatorio %></td>
                <td><label hidden>/Noradrenalina: </label><%=tiss[j].suportecardiovascular %></td>
                <td><label hidden>/Vazão Noradrenalina: </label><%=tiss[j].suporterenal %></td>
                <td><label hidden>/Adrenalina: </label><%=tiss[j].suporteneurologico %></td>
                <td><label hidden>/Vazão Adrenalina: </label><%=tiss[j].suportemetabolico %></td>
                <td><label hidden>/Bicarbonato: </label><%=tiss[j].intervencoes %></td>
                <td><label hidden>/PEEP: </label><%=tiss[j].tiss %></td>
                <td><label hidden>/Dormonid: </label><%=tiss[j].unidade %></td>                             
            </tr>
            <% } %>	
            <% } %>
        </tbody>
    </table>
    <% } %>	
    <% } %>
    <br>
