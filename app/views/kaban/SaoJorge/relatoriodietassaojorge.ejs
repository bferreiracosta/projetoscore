function calcularIdade(aniversario) {
    var ano_atual = new Date().getFullYear();
var data_aniversario = new Date(aniversario).getFullYear();;
// var ano_informado = data_aniversario.split('/')[2];
return (ano_atual - data_aniversario);
   
