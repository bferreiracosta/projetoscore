<% if(id.length > 0) { %>
    <% for(var i = 0; i < id.length; i++) {%>
       
<h2> Fugulin Morumbi</h2>
<a href="addfugulinmorumbi?id=<%=id[i].id_usuario %>"><button class="btn btn-success"  >Atualizar Fugulin</button></a>
<br>
<br>
    <table class="table table-striped" id="mytable">
        <input name="usuario" type="hidden" value="<%=id[i].id_usuario %>">
        
        <thead>
            <tr>
                <th><i class="fas fa-edit"></i></th>
                <th>id</th>
                <th>Nome</th>
                <th>Avaliação Mental</th>
                <th>Avaliação Oxigenação</th>
                <th>Avaliação Sinais Vitais</th>
                <th>Avaliação Motilidade</th>
                <th>Avaliação Deambulação</th>
                <th>Avaliação Alimentação</th>
                <th>Avaliação Cuidado</th>
                <th>Avaliação Eliminação</th>
                <th>Avaliação Terapeutica</th>
                <th>Avaliação Integridade Tissular</th>
                <th>Avaliação Curativo</th>
                <th>Avaliação Tempo Curativo</th>
                <th>Resultado Fugulin</th>
                <th>Unidade</th>  
                <th>Data Atualização</th>   
            </tr>
        </thead>
        <tbody>   
            <% if(fugulin.length > 0) { %>
                <% for(var j = 0; j < fugulin.length; j++) {%>
                    
            <tr>
                <td>
					<a href="editfugulinmorumbi/<%=id[i].id_usuario %>/?id=<%=fugulin[j].idpaciente %>"class="btn btn-sm btn-info edit"> Editar</a>
				</td>
                <td><label for="">ID: </label><%=fugulin[j].idpaciente %></td>
                <td><label hidden for="">Nome: </label><%=fugulin[j].nome %></td>
                <td><label hidden for="">Dispositivo: </label><%=fugulin[j].mental %></td>
                <td><label hidden for="">fluxo_de_o2: </label><%=fugulin[j].oxigenacao %></td>
                <td><label hidden>/Noradrenalina: </label><%=fugulin[j].sinaisvitais %></td>
                <td><label hidden>/Vazão Noradrenalina: </label><%=fugulin[j].motilidade %></td>
                <td><label hidden>/Adrenalina: </label><%=fugulin[j].deambulacao %></td>
                <td><label hidden>/Vazão Adrenalina: </label><%=fugulin[j].alimentacao %></td>
                <td><label hidden>/Bicarbonato: </label><%=fugulin[j].cuidado %></td>
                <td><label hidden>/Vazão Bicarbonato: </label><%=fugulin[j].eliminacao %></td>
                <td><label hidden>/Fio2: </label><%=fugulin[j].terapeutica %></td>
                <td><label hidden>/Fio2: </label><%=fugulin[j].integridade %></td>
                <td><label hidden>/Fio2: </label><%=fugulin[j].curativo %></td>
                <td><label hidden>/Fio2: </label><%=fugulin[j].tempocurativo %></td>
                <td><label hidden>/PEEP: </label><%=fugulin[j].resultado %></td>
                <td><label hidden>/Dormonid: </label><%=fugulin[j].unidade %></td> 
                <td><label hidden>/Dormonid: </label><%=fugulin[j].dataatualizacao %></td>                               
            </tr>
            <% } %>	
            <% } %>
        </tbody>
    </table>
    <% } %>	
    <% } %>
