function setaDadosModal(valor) {
    document.getElementById('campo').value = valor;
}

function setaDadosModal2(valor) {
    document.getElementById('campo2').value = valor;
}
$("#baixaselect").on('change', function(e){
    var baixa = $(this).val();
    document.getElementById('baixa').value = baixa;
});

$("#negativoselect").on('change', function(e){
    var negativo = $(this).val();
    document.getElementById('negativo').value = negativo;
});

function verifica(value){
	var negativoselect = document.getElementById("negativoselect");
	

  if(value == 'Negado'){
    negativoselect.style.display = "block";
  }else{
    negativoselect.style.display = "none";
  } 
};

window.onload = function() {
  document.getElementById('negativoselect').style.display = 'none';
};
</script>