      $('#temp').mask('00.0');
      });

   $("#opcao").on('change', function(e){
     var exame = document.getElementById('opcao').value;
     document.getElementById('ment').value = exame;
});

$("#opcao2").on('change', function(e){
     var o2 = document.getElementById('opcao2').value;
     document.getElementById('uso02').value = o2;
});

function pad(s){
		return (s<10) ? '0' + s :s;
	}
function novaHora(){
	
	var date = new Date();
	document.getElementById('horas').value= [date.getHours(), date.getMinutes()].map(pad).join(':');
	document.getElementById('data').value = date.getDate() + "/" + (date.getMonth()+1) + "/" + date.getFullYear();
}
novaHora();


function calcular(){
	var resultadofr;
	var fr = document.getElementById('fr').value;
	
	if(fr<=8){
		resultadofr = 3;
	}
	if(fr>=9&&fr<=11){
		resultadofr = 1;
	}
	if(fr>=12&&fr<=20){
		resultadofr	= 0;
	}
	if(fr>=21&&fr<=24){
		resultadofr = 2;
	}
	if(fr>=25){
		resultadofr = 3;
	}
	
	var resultadosat;
	var sat = document.getElementById('sat').value;
	
	if(sat<=91){
		resultadosat = 3;
	}
	if(sat>=92&&sat<=93){
		resultadosat = 2;
	}
	if(sat>=94&&sat<95){
		resultadosat	= 1;
	}
	if(sat>=95){
		resultadosat = 0;
	}
	
	var resultadoo2;
	var o2 = document.getElementById('uso02').value;
	
	if(o2 == 'sim'){
		resultadoo2 = 2;
	}
	if(o2 == 'nao'){
		resultadoo2 = 0;
	}
	
	var resultadotemp;
	var temp = document.getElementById('temp').value;
	
	if(temp<=35){
		resultadotemp = 3;
	}
	if(temp>=35.1&&temp<=36){
		resultadotemp = 1;
	}
	if(temp>=36.1&&temp<=38){
		resultadotemp	= 0;
	}
	if(temp>=38.1&&temp<=39){
		resultadotemp = 1;
	}
	if(temp>=39.1){
		resultadotemp = 2;
	}

	var resultadopa;
	var pa = document.getElementById('pa').value;
	
	if(pa<=90){
		resultadopa = 3;
	}
	if(pa>=91&&pa<=100){
		resultadopa = 2;
	}
	if(pa>=101&&pa<=110){
		resultadopa	= 1;
	}
	if(pa>=111&&pa<=219){
		resultadopa = 0;
	}
	if(pa>=220){
		resultadopa = 3;
	}

	
	var resultadofc;
	var fc = document.getElementById('fc').value;
	
	if(fc<=40){
		resultadofc = 3;
	}
	if(fc>=41&&fc<=50){
		resultadofc = 1;
	}
	if(fc>=51&&fc<=90){
		resultadofc	= 0;
	}
	if(fc>=91&&fc<=110){
		resultadofc = 1;
	}
	if(fc>=111&&fc<=130){
		resultadofc = 2;
	}
	if(fc>=131){
		resultadofc = 3;
	}

	
	var resultadoment;
	var ment = document.getElementById('opcao').value;
	
	if(ment == "alerta"){
		resultadoment = 0;
	}
	if(ment == "verbal"){
		resultadoment = 3;
	}
	if(ment == "doloroso"){
		resultadoment = 3;
	}
	if(ment == "aresponsivo"){
		resultadoment = 3;
	}
	
	var score = resultadofr + resultadosat + resultadoo2 + resultadotemp + resultadofc + resultadofc + resultadoment;
	console.log(resultadoment);
	document.getElementById('news').value = score;
	// resultadofc,resultadofc,resultadotemp,resultadoo2,resultadosat,resultadofr
}

</script>